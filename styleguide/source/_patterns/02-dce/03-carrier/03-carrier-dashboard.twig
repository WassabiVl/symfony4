{% block content %}
    {#{% set bgColor = 'primary-color' %}#}
    {#{%  if bgColor is defined %}#}
        {#<div class="fragment {{bgColor}}">#}
    {#{% else %}#}
        {#<div class="fragment {{random(fragmentSetup.colorRange)}}">#}
    {#{% endif %}#}
    {#{% if templateRender == false %}#}
        {#{% include "atoms-colorswitch" %}#}
    {#{% endif %}#}
<div class="fragment carrier-dashboard">
            <div class="column column-100 table-row">
                <div class="column carrier-d-date table-top-bar-item">
                    <span>Lieferung am</span>
                </div>
                <div class="column carrier-d-order table-top-bar-item">
                    <span>Bestell-Nr</span>
                </div>
                <div class="column carrier-d-product table-top-bar-item">
                    <span>Produkt</span>
                </div>
                <div class="column carrier-d-producer table-top-bar-item">
                    <span>Produzent</span>
                </div>
                <div class="column carrier-d-requested-amount table-top-bar-item">
                    <span>Bestellmenge</span>
                </div>
                <div class="column carrier-d-customer table-top-bar-item">
                    <span>Kundenadresse</span>
                </div>
                <div class="column carrier-d-delivered-amount table-top-bar-item">
                    <span>Liefermenge</span>
                </div>
                <div class="column carrier-d-ADR-doc  table-top-bar-item">
                    <span>ADR Dokument</span>
                </div>
        </div>
    {% for i in 0..6 %}
        <div class="column column-100 table-row">
            <div class="column carrier-d-date table-item">
                <span>04.08.2018</span>
            </div>
            <div class="column carrier-d-order table-item">
                <span>NKA25896</span>
            </div>
            <div class="column carrier-d-product table-item">
                <span>Illum</span>
            </div>
            <div class="column carrier-d-producer table-item">
                <span>Blue Kreis Pharma</span>
                <span>Industriestraße 85</span>
                <span>30159 Hannover</span>
            </div>
            <div class="column carrier-d-requested-amount table-item">
                <span>1050 Units</span>
            </div>
            <div class="column carrier-d-customer table-item">
                <span>Dr. Wolfgang Kaiser</span>
                <span>Krankenhausstraße 67</span>
                <span>50674 Köln</span>
            </div>
            <div class="column carrier-d-delivered-amount table-item">
                <span>1058 Units</span>
            </div>
            <div class="column carrier-d-ADR-doc table-item">
                    {#<div class="custom-file-upload">#}
                        <!--<label for="file">File: </label>-->
                        {#<input type="file" id="file" name="myfiles[]" multiple />#}
                    {#</div>#}
                <input type="file" name="file" id="file" class="inputfile" data-multiple-caption="{count} files selected" multiple />
                {#<label for="file">textico</label>#}
                <label for="file"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 8 18"><title>attachment-icon</title><path d="M0,3V14a4,4,0,0,0,8,0V4H7V14a3,3,0,0,1-3,3,3,3,0,0,1-3-3V3A2,2,0,0,1,5,3v9a.94.94,0,0,1-1,1,.94.94,0,0,1-1-1V4H2v8a2,2,0,0,0,4,0V3A3,3,0,0,0,3,0,3,3,0,0,0,0,3Z"/></svg></label>
            </div>
    </div>
    {% endfor %}

</div>
{% endblock %}