{% block content %}
    {#{% set bgColor = 'primary-color' %}#}
    {#{%  if bgColor is defined %}#}
        {#<div class="fragment {{bgColor}}">#}
    {#{% else %}#}
        {#<div class="fragment {{random(fragmentSetup.colorRange)}}">#}
    {#{% endif %}#}
    {#{% if templateRender == false %}#}
        {#{% include "atoms-colorswitch" %}#}
    {#{% endif %}#}
<div class="fragment customer-dashboard">
            <div class="column column-100 table-row">
                <div class="column cd-delivery-date table-top-bar-item">
                    <span>Lieferung am</span>
                </div>
                <div class="column cd-order-date table-top-bar-item">
                    <span>Bestellt am</span>
                </div>
                <div class="column cd-order-number table-top-bar-item">
                    <span>Bestell-Nr</span>
                </div>
                <div class="column cd-order-status table-top-bar-item">
                    <span>Status</span>
                </div>
                <div class="column cd-order-product table-top-bar-item">
                    <span>Produkt</span>
                </div>
                <div class="column cd-order-price table-top-bar-item">
                    <span>Preis</span>
                </div>
                <div class="column cd-order-payment-date table-top-bar-item">
                    <span>Bezahlung Datum</span>
                </div>
                <div class="column cd-order-billing-address table-top-bar-item">
                    <span>Rechnungsadresse</span>
                </div>
                <div class="column cd-order-deliver-address  table-top-bar-item">
                    <span>Lieferadresse</span>
                </div>
                <div class="column cd-order-options table-top-bar-item">
                    <span></span>
                </div>
        </div>
    {% for i in 0..6 %}
        <div class="column column-100 table-row">
            <div class="column cd-delivery-date table-item">
                <span>04.08.2018</span>
            </div>
            <div class="column cd-order-date table-item">
                <span>04.08.2018</span>
            </div>
            <div class="column cd-order-number table-item">
                <span>NKA25896</span>
            </div>
            <div class="column cd-order-status table-item">
                <span class="status-info percentage-details">Teillieferung 40%</span>
                <div class="percentage-bar-wrapper" role="progressbar" aria-valuenow="100" aria-valuemin="0" aria-valuemax="100"><div class="percentage-bar" style="width: 40%;"></div></div>
                <span class="status-info units-details">1000 / 2000 units</span>
            </div>
            <div class="column cd-order-product table-item">
                <span>Illum</span>
            </div>
            <div class="column cd-order-price table-item">
                <span>829.251,50€</span>
            </div>
            <div class="column cd-order-payment-date table-item">
                <span>05.10.2018</span>
            </div>
            <div class="column cd-order-billing-address table-item">
                <span>Geschwister-Scholl-Str. 15</span>
                <span>50674 Köln</span>
            </div>
            <div class="column cd-order-deliver-address table-item">
                <span>Geschwister-Scholl-Str. 15</span>
                <span>50674 Köln</span>
            </div>
            <div class="column cd-order-options table-item">
                <a href="" class="r24-icon order-delete" aria-label="bestell löschen">
